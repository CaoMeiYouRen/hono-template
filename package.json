{
  "name": "hono-template",
  "version": "0.1.0",
  "description": "基于 Hono 和 TypeScript 的云函数模板，支持 Vercel/Cloudflare Workers/Docker/Node.js 等多种部署方式",
  "author": "CaoMeiYouRen",
  "private": true,
  "license": "MIT",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "lint": "cross-env NODE_ENV=production eslint src --fix --ext .ts,.js,.cjs,.mjs",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production tsdown",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "dev:tsup": "cross-env NODE_ENV=development tsdown --watch",
    "start": "node ./dist/index.mjs",
    "start:tsx": "cross-env NODE_ENV=production tsx src/index.ts",
    "start:benchmarks": "cross-env NODE_ENV=test BENCHMARKS_TEST=true node dist/index.mjs",
    "start:benchmarks:bun": "cross-env NODE_ENV=test BENCHMARKS_TEST=true bun dist/bun.mjs",
    "release": "semantic-release",
    "commit": "cz",
    "prepare": "husky install",
    "test": "vitest run",
    "test:benchmarks": "tsx scripts/benchmarks.ts",
    "test:benchmarks:bun": "bun scripts/benchmarks.ts",
    "dev:vercel": "vercel dev",
    "deploy:vercel": "vercel",
    "dev:wrangler": "wrangler dev --env dev",
    "deploy:wrangler": "wrangler deploy",
    "dev:bun": "cross-env NODE_ENV=development bun --watch src/bun.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251202.0",
    "@commitlint/cli": "^20.1.0",
    "@types/autocannon": "^7.12.7",
    "@types/bun": "^1.3.3",
    "@types/debug": "^4.1.12",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@vercel/nft": "^1.1.1",
    "autocannon": "^8.0.0",
    "commitizen": "^4.3.1",
    "commitlint-config-cmyr": "^1.0.0",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog-cmyr": "^2.0.0",
    "debug": "^4.4.3",
    "eslint": "^9.39.1",
    "eslint-config-cmyr": "2.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "octokit": "^5.0.5",
    "rimraf": "^6.1.2",
    "semantic-release": "^25.0.2",
    "semantic-release-cmyr-config": "^1.0.1",
    "tsdown": "^0.17.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vercel": "^49.1.1",
    "vite": "^7.2.6",
    "vitest": "^4.0.15",
    "wrangler": "^4.52.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-cmyr"
    }
  },
  "changelog": {
    "language": "zh"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "keywords": [
    "Hono",
    "TypeScript",
    "Vercel",
    "Docker",
    "Node.js"
  ],
  "homepage": "https://github.com/CaoMeiYouRen/hono-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/hono-template.git"
  },
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/hono-template/issues"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "await-to-js": "^3.0.0",
    "better-bytes": "^1.2.4",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.2",
    "hono": "^4.10.7",
    "isomorphic-unfetch": "^4.0.2",
    "lodash-es": "^4.17.21",
    "tslib": "^2.8.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "pnpm": {
    "overrides": {
      "compare-func": "^2.0.0",
      "js-yaml": "4.1.1",
      "tar": "7.5.2",
      "form-data": "^4.0.4",
      "@conventional-changelog/git-client": "^2.5.1",
      "glob": "^11.1.0",
      "undici": "^5.29.0",
      "esbuild": "^0.25.12",
      "path-to-regexp": "^6.3.0",
      "tmp": "^0.2.4"
    }
  },
  "type": "module"
}
